msgstr ""

#. type: Plain text
#: docs/cli.md:32
msgid ""
"This command will help you kickstart your new Python project by creating a "
"directory structure suitable for most projects."
msgstr ""

#. type: Plain text
#: docs/cli.md:33
#, no-wrap
msgid ""
"poetry new my-package\n"
"\n"
msgstr ""

#. type: Plain text
#: docs/cli.md:38
msgid "will create a folder as follows:"
msgstr ""

#. type: Code fence info string
#: docs/cli.md:39 docs/cli.md:65
#, no-wrap
msgid "text"
msgstr ""

#. type: Plain text
#: docs/cli.md:39
#, no-wrap
msgid ""
"my-package\n"
"\n"
"├── pyproject.toml\n"
"\n"
"├── README.rst\n"
"\n"
"├── my_package\n"
"\n"
"│   └── __init__.py\n"
"\n"
"└── tests\n"
"\n"
"    ├── __init__.py\n"
"\n"
"    └── test_my_package.py\n"
"\n"
